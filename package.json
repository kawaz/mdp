{
  "name": "@kawaz/mdp",
  "version": "0.1.0",
  "description": "Terminal Markdown Preview CLI powered by MoonBit",
  "bin": {
    "mdp": "./bin/mdp.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "moon build --target js && bun build _build/js/release/build/src/src.js --outfile=dist/mdp.js --minify --target=node",
    "test": "moon test --target js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "markdown",
    "terminal",
    "cli",
    "tui",
    "preview",
    "moonbit",
    "ansi"
  ],
  "author": "kawaz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kawaz/mdp.git"
  },
  "bugs": {
    "url": "https://github.com/kawaz/mdp/issues"
  },
  "homepage": "https://github.com/kawaz/mdp#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
